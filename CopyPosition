using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CopyPosition : MonoBehaviour
{
    public Transform preFab;

    public Transform target;
    public float speed;

    
    void Update()
    {
        if (preFab != null)
        {
            if (target != null)
            {

                transform.position = target.position;
            }
        }
    }
}
