using UnityEngine;

public class WhileLoopTest : MonoBehaviour {
    public int i = 1;

	// Use this for initialization
	void Start () {
        while (i<6) {
            Debug.Log("the value of i is " + i);
            i++;
        }
	}
	
	// Update is called once per frame
	void Update () {
		
	}
}
